		
<div class="got_vtag wrapper">
	<h2><%= @page_title %></h2>
			<div class="got_vtag">
				<!--<div class="left_sidebar"></div>-->
				<ul id="video_list">
					<% @videos.each do |v| %>
						<% user = v.user %>
						<li>
							<!--<div class="video_div">-->
								<div class="video_thumb"><a href="/fb/<%= v.fb_id %>"><img src="<%= v.thumb_small_src %>" width="90" height="48" alt="<%= v["name"] %>" /></a></div>
								<div class="video_title"><a href="/fb/<%= v.fb_id %>"><%= v.title %></a></div>
								<div class="video_by"><a href="http://www.facebook.com/<%= user.fb_id %>"><%=fb_profile_pic("23x23", user.nick, user.id)  %>By: <%= user.nick %></a></div>
								
							<!--</div>-->
						</li>
					<% end %>
				</ul>
			</div>	
</div>
